

class NavBoxCon extends Component {
	render(){
		let boxData = this.props.data;  {/*如果有的话就是个数组，没有的话为undefined*/}
		let boxLi = boxData.map(function(data){			
			return(
				<li key={data.id} className="box-li">
					<a href="#">{data}</a>
				</li>
			)
		})
		return (
			<div className="nav-box-wrap"><ul>{boxLi}</ul></div>
		)
	}

}

